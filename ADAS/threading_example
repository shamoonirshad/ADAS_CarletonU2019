import _thread

def thread():
	global count
	count = 0
	print("I am a dumb thread")
	count += 1
	
try:
	_thread.start_new_thread(thread, ())
	_thread.start_new_thread(thread, ())
	_thread.start_new_thread(thread, ())
	_thread.start_new_thread(thread, ())
	_thread.start_new_thread(thread, ())
	

except:
	print("FAIL")
	
while 1:
	pass
